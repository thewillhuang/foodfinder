{
  "name": "foodfinder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run version && npm run bower && npm run gulpBuild && npm run rebuildSass",
    "version": "node -v",
    "bower": "bower install --config.interactive=false",
    "gulpBuild": "cd gulp && npm install",
    "rebuildSass": "cd gulp/ && cd node_modules/gulp-sass/ && npm uninstall node-sass && npm install node-sass",
    "build": "cd gulp && NODE_ENV=development && gulp",
    "start": "node server",
    "production": "cd gulp && NODE_ENV=production && gulp production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WillM88/FooFinder.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WillM88/FooFinder/issues"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  },
  "homepage": "https://github.com/WillM88/FooFinder",
  "dependencies": {
    "body-parser": "^1.12.2",
    "bootstrap": "^3.3.4",
    "bower": "^1.4.1",
    "compression": "^1.4.3",
    "express": "^4.12.3",
    "flux": "^2.0.1",
    "jquery": "~2.1.0",
    "mongoose": "^4.0.1",
    "morgan": "^1.5.2",
    "react": "^0.13.2",
    "react-bootstrap": "^0.21.0"
  },
  "devDependencies": {
    "reactify": "^1.1.0"
  }
}
