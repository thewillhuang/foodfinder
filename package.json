{
  "name": "foodfinder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run bower && npm run version && npm run production",
    "bower": "bower install --config.interactive=false",
    "version": "node -v",
    "rebuildSass": "cd node_modules/gulp-sass/ && npm uninstall node-sass && npm install node-sass",
    "build": "cd gulp && npm install && NODE_ENV=development && gulp",
    "start": "node server",
    "test": "npm run production",
    "production": "cd gulp && npm install && NODE_ENV=production && gulp production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WillM88/FooFinder.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WillM88/FooFinder/issues"
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "homepage": "https://github.com/WillM88/FooFinder",
  "dependencies": {
    "body-parser": "^1.12.2",
    "bootstrap": "^3.3.4",
    "bower": "^1.4.1",
    "express": "^4.12.3",
    "mongoose": "^4.0.1",
    "morgan": "^1.5.2",
    "jquery": "~2.1.0"
  },
  "devDependencies": {
    "browser-sync": "~2.2.2",
    "browserify": "^9.0.3",
    "browserify-shim": "^3.8.2",
    "coffeeify": "~1.0.0",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-cached": "^1.0.4",
    "gulp-changed": "^1.1.1",
    "gulp-filesize": "0.0.6",
    "gulp-iconfont": "^1.0.0",
    "gulp-imagemin": "^2.2.1",
    "gulp-minify-css": "~0.5.1",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "~1.3.3",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-swig": "^0.7.4",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "handlebars": "^3.0.0",
    "hbsfy": "~2.2.1",
    "karma": "^0.12.31",
    "karma-browserify": "^4.0.0",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coffee-preprocessor": "^0.2.1",
    "karma-mocha": "^0.1.10",
    "karma-nyan-reporter": "0.0.51",
    "karma-sinon-chai": "^0.3.0",
    "lodash": "^3.3.1",
    "merge-stream": "^0.1.7",
    "node-neat": "^1.7.1-beta1",
    "pretty-hrtime": "~1.0.0",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "^2.4.0"
  }
}
